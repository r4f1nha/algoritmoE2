#include <stdio.h>
#define TAM 18

double valorMinuto(int n) {


     if(n==5) {
        return (7.50);
    }
    if(n==10) {
        return (15.0);
    }
    if(n==15) {
        return (22.50);
    }
    if(n==20) {
        return (30.0);
    }
    if(n==25) {
        return (37.50);
    }
    if(n==30) {
        return (45.0);
    }
    if(n==35) {
        return (52.50);
    }
    if(n==40) {
        return (60.0);
    }
    if(n==45) {
        return (67.50);
    }
    if(n==50) {
        return (75.50);
    }
    if(n==55) {
        return (82.50);
    }
    if(n==60) {
        return (90.0);
    }
     if(n==90) {
        return (135.0);
    }
     if(n==100) {
        return (150.0);
    }
     if(n==120) {
        return (180.0);
    }
    if(n==180) {
        return (225.0);
    }
    
}

int main(){
 
    int n;
    
    while (n=!0) {
    scanf("%d",&n);
 
    printf("%f \n" , valorMinuto(n));
     
}
